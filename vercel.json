{
  "version": 2,
  "builds": [
    {
      "src": "GadgetsCorner/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "GadgetsCorner/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "GadgetsCorner/wsgi.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "DJANGO_SETTINGS_MODULE": "GadgetsCorner.settings" 
  },
  "functions": {
    "runtime": "python3.9"
  }
}